<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rf.ui.MainController">
   <children>
      <TextField fx:id="sessionIdText" />
      <Button fx:id="getDataBtn" mnemonicParsing="false" onAction="#getRejected" text="GET!" />
      <Label text="Rejected:" />
      <TextArea fx:id="rejectedListText" editable="false" wrapText="true" VBox.vgrow="ALWAYS" />
      <Label fx:id="statusLabel" text="Status: " />
   </children>
</VBox>
