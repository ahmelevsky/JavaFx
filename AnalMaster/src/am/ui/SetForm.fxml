<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="600.0" minWidth="1000.0" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="am.ui.SetController">
   <children>
      <HBox VBox.vgrow="NEVER">
         <children>
            <Pane HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" VBox.vgrow="ALWAYS">
         <items>
            <HBox>
               <children>
                  <TableView fx:id="tableView" HBox.hgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="columnName" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Set Name">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                        <TableColumn fx:id="columnStartDate" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Start Upload">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                        <TableColumn fx:id="columnLastDate" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Last Upload">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                        <TableColumn fx:id="columnImagesCount" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Images Count">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                        <TableColumn fx:id="columnSoldImagesCount" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Sold Images Count">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                        <TableColumn fx:id="columnSoldPercent" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Sold Percent">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                        <TableColumn fx:id="columnSum" editable="false" maxWidth="1000.0" minWidth="20.0" prefWidth="200.0" sortable="false">
                           <graphic>
                              <VBox spacing="10.0">
                                 <children>
                                    <Label text="Sum, $">
                                       <VBox.margin>
                                          <Insets left="5.0" top="5.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </graphic>
                        </TableColumn>
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </HBox>
            <HBox>
               <children>
                  <LineChart fx:id="lineChart" HBox.hgrow="ALWAYS">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
               </children>
            </HBox>
         </items>
      </SplitPane>
   </children>
</VBox>
