<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="te.view.VariablesEditorContainerController">
   <children>
      <AnchorPane prefHeight="50.0">
         <children>
            <Label layoutX="15.0" layoutY="1.0" prefHeight="21.0" prefWidth="159.0" text="%ui.tabs.varcont.name">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Label layoutX="209.0" layoutY="1.0" prefHeight="21.0" prefWidth="145.0" text="%ui.tabs.varcont.devider">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Label layoutX="438.0" layoutY="1.0" prefHeight="21.0" prefWidth="281.0" text="%ui.tabs.varcont.values">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Label layoutX="921.0" layoutY="1.0" prefHeight="21.0" prefWidth="76.0" text="%ui.tabs.varcont.delete">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="450.0">
         <content>
            <VBox fx:id="variableLayouts" />
         </content>
      </ScrollPane>
      <BorderPane>
         <left>
            <Button fx:id="addVarBtn" mnemonicParsing="false" onAction="#addVariableLayout" stylesheets="@../application.css" text="+" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </Button>
         </left>
         <right>
               </right>
      </BorderPane>
   </children>
</VBox>
